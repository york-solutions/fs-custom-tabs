{
  "manifest_version": 2,

  "name": "FS Custom Tabs",
  "description": "Add custom tabs to the FamilySearch Family Tree",
  "version": "1.0",

  "content_scripts": [
    {
      "matches": ["https://familysearch.org/tree/*"],
      "css": ["src/index.css"],
      "js": [
        "src/AvailableTabs.js",
        "src/Base.js",
        "src/ConfigTab.js",
        "src/ConfigTabContent.js",
        "src/CustomTab.js",
        "src/OriginalController.js",
        "src/index.js"
      ]
    }
  ]
}